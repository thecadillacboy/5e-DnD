    "import random\n",
    "\n",
    "# Returns a list of rolled die and their sum. Index 0 is sum. Index 1 is list\n",
    "def ndicen(count,sides) :\n",
    "    dice = []\n",
    "    for die in range(count) :\n",
    "        pips = random.randint(1, sides)\n",
    "        dice.append(pips)\n",
    "    return(sum(dice), dice)"
   ]
